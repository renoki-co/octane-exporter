{
    "name": "renoki-co/octane-exporter",
    "description": "Export Laravel Octane metrics using this Prometheus exporter.",
    "keywords": ["laravel", "php", "octane", "swoole", "metrics", "grafana", "prometheus"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/octane-exporter",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/octane": "^1.0",
        "renoki-co/laravel-exporter-contracts": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\OctaneExporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\OctaneExporter\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^6.24",
        "orchestra/testbench-core": "^6.28"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiCo\\OctaneExporter\\OctaneExporterServiceProvider"
            ]
        }
    }
}
